  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@bytecodealliance/componentize-js": "^0.7.0",
    "@bytecodealliance/jco": "^1.0",
    "@types/node": "^20.10.8",
    "babel-loader": "^9.1.3",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "dependencies": {
    "cowsayjs": "^2.0.0",
    "timers": "^0.1.1"
  }
